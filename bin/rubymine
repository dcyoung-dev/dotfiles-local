#!/bin/bash
#Generated by JetBrains Toolbox 2.1.1.18388 at 2023-11-29T09:18:16.443133

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/davidyoung/Applications/RubyMine.app/Contents/MacOS/rubymine" -W $wait --args "${intellij_args[@]}"