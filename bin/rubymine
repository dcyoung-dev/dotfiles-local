#!/bin/bash
#Generated by JetBrains Toolbox 2.5.2.35332 at 2024-12-13T23:16:51

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/dcyoung/Applications/RubyMine.app/Contents/MacOS/rubymine" $wait --args "${intellij_args[@]}"